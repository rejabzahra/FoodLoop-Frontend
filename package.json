{
  "name": "food-loop",
  "version": "1.0.0",
  "description": "FOOD LOOP - Connecting surplus food with those in need",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build"
  },
  "keywords": [
    "food",
    "donation",
    "community",
    "zero-waste"
  ],
  "author": "FOOD LOOP Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.2",
    "react-scripts": "^5.0.1",
    "sqlite3": "^5.0.2"
  }
}
